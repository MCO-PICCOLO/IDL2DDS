@final
struct VehicleStatus {
    // Vehicle Status Information
    long speed;          // Current speed in km/h
    long rpm;            // Engine RPM
    float fuelLevel;     // Fuel level (0-100%)
    boolean engineOn;    // Engine status
    long gear;           // Current gear (0=P, 1=R, 2=N, 3-10=D1-D8)
};
